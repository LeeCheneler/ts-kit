{
  "name": "@leecheneler/ts-kit",
  "version": "1.0.0",
  "description": "Zero config CLI for writing TypeScript packages.",
  "repository": {
    "url": "https://github.com/leecheneler/ts-kit"
  },
  "license": "MIT",
  "bin": {
    "ts-kit": "./dist/cli.js"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.build.json && cp -R src/tool-files dist",
